@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<header>
    <div class="logout-page">
        <div class="lead">
            <h1>Logout</h1>
            <p>Would you like to logout of Todoist?</p>
        </div>

        <form method="post">
            <div class="form-group">
                <button class="btn btn-primary">Yes</button>
            </div>
        </form>
    </div>
</header>